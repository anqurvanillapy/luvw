{
  "name": "luvw",
  "version": "0.1.0",
  "description": "Dependency-free 2-way data binding microlibrary",
  "main": "index.js",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register",
    "build": "mkdir -p dist && babel --no-babelrc --presets=babili index.js -o dist/luvw.min.js",
    "watch": "mkdir -p dist && babel index.js -o dist/luvw.min.js --watch",
    "clean": "rm -rf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/anqurvanillapy/luvw.git"
  },
  "keywords": [
    "mvvm",
    "observable",
    "two-way-data-binding"
  ],
  "author": "AnqurVanillapy",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/anqurvanillapy/luvw/issues"
  },
  "homepage": "https://github.com/anqurvanillapy/luvw#readme",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.20.0",
    "babel-preset-babili": "0.0.9",
    "babel-preset-es2015": "^6.18.0",
    "expect.js": "^0.3.1",
    "mocha": "^3.2.0"
  }
}
